{
  "seed": 1337,
  "device": "cpu",
  "ipfe": {
    "prime": 1000000007,
    "encryption_length": 9
  },
  "data": {
    "name": "MNIST",
    "root": "./data",
    "batch_size": 64,
    "num_workers": 2,
    "download": true,
    "max_test_samples": 10
  },
  "model": {
    "name": "plain",
    "in_channels": 1,
    "num_classes": 10,
    "c1": 16,
    "c2": 32,
    "c3": 64,
    "k1": 3,
    "k2": 3,
    "k3": 3,
    "dropout": 0.5
  },
  "optimizations": {
    "precrypted": true,
    "optimized_ipfe": true,
    "kernel_parallelization": false,
    "kernel_patches_parallelization": false,
    "batch_parallelization": false,
    "batch_kernels_parallelization": true
  },
  "train": {
    "epochs": 3,
    "lr": 0.0005,
    "optimizer": "adam",
    "print_every": 200
  },
  "save": {
    "out_dir": "PrivacyPreservingML/results",
    "run_name": "mnist"
  }
}